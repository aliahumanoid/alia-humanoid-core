{
  "version": "0.2",
  "language": "en",
  "enabled": false,
  "diagnosticLevel": "Hint",
  "showSuggestions": false,
  "ignorePaths": [
    "**/.git/**",
    "**/node_modules/**",
    "**/venv/**",
    "**/.venv/**",
    "**/build/**",
    "**/dist/**",
    "**/__pycache__/**",
    "**/logs/**",
    "**/mapping_data/**",
    "**/*.log",
    "**/*.bin",
    "**/*.hex",
    "**/*.elf",
    "**/*.map"
  ],
  "words": [
    "alia",
    "humanoid",
    "pico",
    "platformio",
    "arduino",
    "cpp",
    "hpp",
    "gpio",
    "spi",
    "uart",
    "pwm",
    "adc",
    "dac",
    "i2c",
    "can",
    "usb",
    "mcp",
    "mt6835",
    "lkm",
    "pid",
    "dof",
    "dofs",
    "agonist",
    "antagonist",
    "extensor",
    "flexor",
    "pretension",
    "pretensioning",
    "tendon",
    "encoder",
    "encoders",
    "millidegrees",
    "microcontroller",
    "firmware",
    "bootloader",
    "watchdog",
    "mutex",
    "semaphore",
    "fifo",
    "lifo",
    "endstop",
    "homing",
    "calibration",
    "interpolation",
    "extrapolation",
    "spline",
    "bezier",
    "quintic",
    "cubic",
    "trajectory",
    "kinematics",
    "dynamics",
    "torque",
    "voltage",
    "amperage",
    "milliamps",
    "millivolts",
    "microseconds",
    "milliseconds",
    "nanoseconds",
    "hertz",
    "khz",
    "mhz",
    "ghz"
  ],
  "flagWords": [],
  "overrides": [
    {
      "filename": "**/*.{cpp,h,hpp,c}",
      "enabled": false
    },
    {
      "filename": "**/*.{py,pyx,pyi}",
      "enabled": false
    },
    {
      "filename": "**/*.{md,txt,rst}",
      "enabled": false
    },
    {
      "filename": "**/*.{json,yaml,yml,toml,ini,cfg}",
      "enabled": false
    }
  ]
}
